{
    "manifest_version": 2,

    "name": "Deep Back",
    "description": "This extension gives ability to go back by skiping all pages from the current web site.",
    "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
    "version": "1.0",
    "background": { "scripts": ["deepback.js"] },

    "browser_action": {
    },

    "permissions": [
        "tabs",
        "notifications",
        "webNavigation",
        "storage",
        "history",
        "http://*/*",
        "https://*/*"
    ]
}
